	&#xd;
	var atividadeAtual = getValue(&quot;WKCurrentState&quot;);&#xd;
	&#xd;
	if(atividadeAtual == atv_aprovacao){&#xd;
		var assunto 		= &quot;Solicitação de Inclusão de Gratificação&quot;;&#xd;
