		var destinatario 	= new java.util.ArrayList(); &#xd;
		destinatario.add(colleagueId);&#xd;
		destinatario.add(hAPI.getCardValue(&apos;email&apos;));&#xd;
		log.info(&quot;----Destinatarios &quot; + destinatario)&#xd;
